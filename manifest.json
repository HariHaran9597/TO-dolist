{
  "manifest_version": 2,
  "name": "My To-Do List",
  "version": "1.0",
  "description": "A feature-rich to-do list Chrome extension",
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
